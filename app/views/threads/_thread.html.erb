<div id="<%= dom_id thread %>">
  <p>
    <strong>Title:</strong>
    <%= thread.title %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= thread.content %>
  </p>

  <p>
    <strong>User:</strong>
    <%= thread.user_id %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= thread.category_id %>
  </p>

</div>
